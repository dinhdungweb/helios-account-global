{% doc %}
  @prompt
    Please code a timeline section for me with a vertical layout, one side for text and the other for media (which could be an image or video), with interchangeable options. Optimize the code for fast page load times. The text column should have five characters at the top and a description at the bottom. Add a line in the middle to mark the timestamps. I have about 20 timestamps, Phần video cho chọn giống như ảnh, và ở trên mobile cho hiển thị ảnh trước label và text ở dưới, Phần video cho chọn giống như ảnh, và ở trên mobile cho hiển thị ảnh trước label và text ở dưới, Thêm lại tùy chọn chọn video đi., Chọn video hoặc ảnh, Video dạng picker chứ không phải dán URL, Cho tôi thêm tùy chọn bật tắt auto play video và, tùy chỉnh độ rộng của section.
{% enddoc %}
{% assign block_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .section-timeline-{{ block_id }} {
    width: 100%;
    max-width: {{ block.settings.section_width }}px;
    margin: 0 auto;
    padding: {{ block.settings.section_padding }}px 20px;
    background-color: {{ block.settings.background_color }};
  }

  .section-timeline-header-{{ block_id }} {
    text-align: center;
    margin-bottom: 60px;
  }

  .section-timeline-title-{{ block_id }} {
    font-size: {{ block.settings.title_size }}px;
    color: {{ block.settings.title_color }};
    margin: 0 0 16px;
  }

  .section-timeline-subtitle-{{ block_id }} {
    font-size: {{ block.settings.subtitle_size }}px;
    color: {{ block.settings.subtitle_color }};
    margin: 0;
  }

  .section-timeline-list-{{ block_id }} {
    position: relative;
    list-style: none;
    padding: 0;
    margin: 0;
  }

  .section-timeline-list-{{ block_id }}::before {
    content: '';
    position: absolute;
    left: 50%;
    top: 0;
    bottom: 0;
    width: {{ block.settings.line_width }}px;
    background-color: {{ block.settings.line_color }};
    transform: translateX(-50%);
  }

  .section-timeline-item-{{ block_id }} {
    position: relative;
    margin-bottom: 80px;
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 60px;
    align-items: center;
  }

  .section-timeline-item-{{ block_id }}:nth-child(even) .section-timeline-content-{{ block_id }} {
    order: 2;
  }

  .section-timeline-item-{{ block_id }}:nth-child(even) .section-timeline-media-{{ block_id }} {
    order: 1;
  }

  .section-timeline-marker-{{ block_id }} {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    width: {{ block.settings.marker_size }}px;
    height: {{ block.settings.marker_size }}px;
    background-color: {{ block.settings.marker_color }};
    border: {{ block.settings.marker_border_width }}px solid {{ block.settings.marker_border_color }};
    border-radius: 50%;
    z-index: 2;
  }

  .section-timeline-content-{{ block_id }} {
    padding: 20px;
  }

  .section-timeline-label-{{ block_id }} {
    font-size: {{ block.settings.label_size }}px;
    color: {{ block.settings.label_color }};
    font-weight: 600;
    margin: 0 0 12px;
    letter-spacing: 0.5px;
  }

  .section-timeline-description-{{ block_id }} {
    font-size: {{ block.settings.description_size }}px;
    color: {{ block.settings.description_color }};
    line-height: 1.6;
    margin: 0;
  }

  .section-timeline-media-{{ block_id }} {
    position: relative;
    width: 100%;
    border-radius: {{ block.settings.media_border_radius }}px;
    overflow: hidden;
  }

  .section-timeline-media-{{ block_id }} img {
    width: 100%;
    height: auto;
    display: block;
    border-radius: {{ block.settings.media_border_radius }}px;
  }

  .section-timeline-media-{{ block_id }} video {
    width: 100%;
    height: auto;
    display: block;
    border-radius: {{ block.settings.media_border_radius }}px;
  }

  .section-timeline-media-placeholder-{{ block_id }} {
    position: relative;
    width: 100%;
    aspect-ratio: 16 / 9;
    background-color: #f4f4f4;
    border-radius: {{ block.settings.media_border_radius }}px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .section-timeline-media-placeholder-{{ block_id }} svg {
    width: 60%;
    height: 60%;
    opacity: 0.3;
  }

  @media screen and (max-width: 768px) {
    .section-timeline-list-{{ block_id }}::before {
      left: 0px;
    }

    .section-timeline-item-{{ block_id }} {
      grid-template-columns: 1fr;
      gap: 20px;
      padding-left: 20px;
      margin-bottom: 60px;
    }

    .section-timeline-item-{{ block_id }}:nth-child(even) .section-timeline-content-{{ block_id }},
    .section-timeline-item-{{ block_id }} .section-timeline-content-{{ block_id }} {
      order: 2;
    }

    .section-timeline-item-{{ block_id }}:nth-child(even) .section-timeline-media-{{ block_id }},
    .section-timeline-item-{{ block_id }} .section-timeline-media-{{ block_id }} {
      order: 1;
    }

    .section-timeline-marker-{{ block_id }} {
      left: 0px;
      top: 30px;
      transform: translate(-50%, 0);
    }

    .section-timeline-content-{{ block_id }} {
      padding: 0;
    }
  }
{% endstyle %}

<div class="section-timeline-{{ block_id }}" {{ block.shopify_attributes }}>
  {% if block.settings.section_title != blank or block.settings.section_subtitle != blank %}
    <div class="section-timeline-header-{{ block_id }}">
      {% if block.settings.section_title != blank %}
        <h2 class="section-timeline-title-{{ block_id }}">{{ block.settings.section_title }}</h2>
      {% endif %}
      {% if block.settings.section_subtitle != blank %}
        <p class="section-timeline-subtitle-{{ block_id }}">{{ block.settings.section_subtitle }}</p>
      {% endif %}
    </div>
  {% endif %}

  <ul class="section-timeline-list-{{ block_id }}">
    {% for i in (1..20) %}
      {% liquid
        assign item_enabled_key = 'item_' | append: i | append: '_enabled'
        assign item_label_key = 'item_' | append: i | append: '_label'
        assign item_description_key = 'item_' | append: i | append: '_description'
        assign item_media_type_key = 'item_' | append: i | append: '_media_type'
        assign item_image_key = 'item_' | append: i | append: '_image'
        assign item_video_key = 'item_' | append: i | append: '_video'

        assign item_enabled = block.settings[item_enabled_key]
        assign item_label = block.settings[item_label_key]
        assign item_description = block.settings[item_description_key]
        assign item_media_type = block.settings[item_media_type_key]
        assign item_image = block.settings[item_image_key]
        assign item_video = block.settings[item_video_key]
      %}

      {% if item_enabled %}
        <li class="section-timeline-item-{{ block_id }}">
          <div class="section-timeline-marker-{{ block_id }}"></div>

          <div class="section-timeline-content-{{ block_id }}">
            {% if item_label != blank %}
              <div class="section-timeline-label-{{ block_id }}">{{ item_label }}</div>
            {% endif %}
            {% if item_description != blank %}
              <div class="section-timeline-description-{{ block_id }}">{{ item_description }}</div>
            {% endif %}
          </div>

          <div class="section-timeline-media-{{ block_id }}">
            {% if item_media_type == 'video' and item_video %}
              {{
                item_video
                | video_tag:
                  image_size: '800x',
                  autoplay: block.settings.video_autoplay,
                  loop: block.settings.video_loop,
                  controls: true,
                  muted: block.settings.video_muted,
                  loading: 'lazy'
              }}
            {% elsif item_media_type == 'image' and item_image %}
              <img
                src="{{ item_image | image_url: width: 800 }}"
                alt="{{ item_label | escape }}"
                width="800"
                height="{{ 800 | divided_by: item_image.aspect_ratio | round }}"
                loading="lazy"
              >
            {% else %}
              <div class="section-timeline-media-placeholder-{{ block_id }}">
                {{ 'image' | placeholder_svg_tag }}
              </div>
            {% endif %}
          </div>
        </li>
      {% endif %}
    {% endfor %}
  </ul>
</div>

